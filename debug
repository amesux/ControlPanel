;---- Window Wait

waitForWindow(neededWindow){
	Sleep(200)
	WinActivate(neededWindow)
	WinWaitActive(neededWindow)
	Sleep(200)
}	

;---- Move Active Window

MoveBitch(mon, win := "") {
    if (!win)
        win := WinActive()
    if !WinGetPos(,,,&w,&h, win)
        return false
    x := mon.Left + ((mon.Right - mon.Left - w) // 2)
    y := mon.Top + ((mon.Bottom - mon.Top - h) // 2)
    return WinMove(win, "", x, y, w, h)
}
    ; Get target monitor's position
    MonitorGet(monitorNumber, &left, &top, &right, &bottom)
    width := right - left
    height := bottom - top

    ; Get the active window
    win := WinExist("A")
    if !win {
        MsgBox "No active window found."
        return
    }

StepWiseWait(){
	neededWindow := "Stepwise ahk_exe Stepwise.exe"
  	WinWaitActive(neededWindow)
}

IllustratorWait(){
	neededWindow := "ahk_exe Illustrator.exe"
	WinActivate(neededWindow)
	WinWaitActive(neededWindow)
}

PhotoshopWait(){
	neededWindow := "ahk_exe Photoshop.exe"
	WinActivate(neededWindow)
	WinWaitActive(neededWindow)
}

RunFolder(folderNeeded){
	neededWindow := "ahk_exe explorer.exe"
	Run(folderNeeded)
	WinActivate(neededWindow)
	WinWaitActive(neededWindow)
}

RunFile(fileNeeded){
	SplitPath fileNeeded, , , &ext
	if ext = "txt"
		neededWindow := "ahk_exe notepad.exe"
		Run(fileNeeded)
		WinWaitActive(neededWindow)
	else if ext = "ai"
		neededWindow := "ahk_exe Illustrator.exe"
		Run(fileNeeded)
		WinWaitActive(neededWindow)
	else
		Sleep(200)
	MoveBitch(monitor2)
}

