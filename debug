Checklist(){
targetTitle := "Temp Checklist"
url := "https://docs.google.com/spreadsheets/d/1HRnLhbKjzSY0IxPE4crcgiSVFyVtP1UBvtSNrk2-L9s/edit#gid=177519100"

if WinExist("Temp Checklist - Google Sheets ahk_class Mozilla Window Class ahk_exe firefox.exe"){
WinActivate("Temp Checklist - Google Sheets ahk_class Mozilla Window Class ahk_exe firefox.exe")
MoveBitch(monitor1)
return
}

found := false

for hwnd in WinGetList("ahk_class MozillaWindowClass") {
	title := WinGetTitle(hwnd)
	if InStr(title, targetTitle){
   	WinActivate(hwnd)
   	msgGui := Gui()
	msgGui.Add("Text", ,"Window Activated!")
	msgGui.Show()
	SetTimer(() => msgGui.Destroy(), 1000)
   	 found := true
  	  break
   	 }
   }
	if !found {
   	 Run ("https://docs.google.com/spreadsheets/d/1HRnLhbKjzSY0IxPE4crcgiSVFyVtP1UBvtSNrk2-L9s/edit?gid=177519100#gid=177519100")
	}
}

