evtHandlers := ["SendPath", "OpenMUA", "OpenTemplate", "SaveMaster", "alignX", "alignY", "alignL", "alignR", "rotate180", "flipX", "flipY", "layersRem", "layersForg", "psQuickFill", "objSelectExact"]
